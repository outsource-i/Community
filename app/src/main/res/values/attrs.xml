<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- infofragment页面的自定义组合View-->
    <declare-styleable name="InfoItemView">
        <attr name="item_info_icon" format="reference"/>
        <attr name="item_info" format="string"/>
    </declare-styleable>
    <declare-styleable name="AuthView">
        <attr name="item_auth_icon" format="reference"/>
        <attr name="item_auth" format="string"/>
    </declare-styleable>
    <declare-styleable name="ZImageView">
        <attr name="z_style" format="string">
            <enum name="oval" value="1"/>
            <enum name="rect" value="2"/>
        </attr>
        <attr name="z_radius" format="integer" />
        <attr name="z_fitxy" format="boolean" />
    </declare-styleable>
    <declare-styleable name="roundView">
        <attr format="reference|color" name="backgroundColor" />
    </declare-styleable>
    <!-- 借款详情页面页面的自定义组合View-->
    <declare-styleable name="BorrowDetailView">
        <attr name="borrow_name" format="string"></attr>
        <attr name="borrow_color" format="color"></attr>
        <attr name="borrow_gone" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="VerticalScrollView">
        <attr name="v_yspeed" format="float"/>
        <attr name="v_xspeed" format="float"/>
        <attr name="v_interval" format="integer"/>
        <attr name="v_textColor" format="color"/>
        <attr name="v_textSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="SelfItemView">
        <attr name="self_line" format="boolean"></attr>
        <attr name="self_title" format="string"></attr>
    </declare-styleable>
    <!--swipe toloadmore-->
    <declare-styleable name="SwipeToLoadLayout">
        <attr name="refresh_enabled" format="boolean" />
        <attr name="load_more_enabled" format="boolean" />

        <attr name="swipe_style" format="enum">
            <enum name="classic" value="0" />
            <enum name="above" value="1" />
            <enum name="blew" value="2" />
            <enum name="scale" value="3" />
        </attr>

        <attr name="drag_ratio" format="float" />

        <attr name="refresh_trigger_offset" format="dimension" />
        <attr name="load_more_trigger_offset" format="dimension" />
        <attr name="refresh_final_drag_offset" format="dimension" />
        <attr name="load_more_final_drag_offset" format="dimension" />

        <attr name="swiping_to_refresh_to_default_scrolling_duration" format="integer" />
        <attr name="release_to_refreshing_scrolling_duration" format="integer" />
        <attr name="refresh_complete_delay_duration" format="integer" />
        <attr name="refresh_complete_to_default_scrolling_duration" format="integer" />
        <attr name="default_to_refreshing_scrolling_duration" format="integer" />
        <attr name="swiping_to_load_more_to_default_scrolling_duration" format="integer" />
        <attr name="release_to_loading_more_scrolling_duration" format="integer" />
        <attr name="load_more_complete_delay_duration" format="integer" />
        <attr name="load_more_complete_to_default_scrolling_duration" format="integer" />
        <attr name="default_to_loading_more_scrolling_duration" format="integer" />
    </declare-styleable>
    <declare-styleable name="MutipleLayout">
        <attr name="isFirstVisible" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SeekView">
        <attr name="seek_lineHeight" format="dimension"/>
        <attr name="seek_thumbRadius" format="dimension"/>
        <attr name="seek_thumbWaiRadius" format="dimension"/>
        <attr name="seek_thumbRadius_color" format="color"/>
        <attr name="seek_thumbWaiRadius_moveColor" format="color"/>
        <attr name="seek_thumbWaiRadius_stopColor" format="color"/>
        <attr name="seek_tips_textsize" format="dimension"/>
        <attr name="seek_tickmark_textsize" format="dimension"/>
        <attr name="seek_tickmark_width" format="dimension"/>
        <attr name="seek_tickmark_height" format="dimension"/>
        <attr name="seek_tickmark_thumb_offset" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>
    <declare-styleable name="EvaluateButton">
        <attr name="eva_bg_press" format="reference" />
        <attr name="eva_bg_normal" format="reference" />
    </declare-styleable>
    <declare-styleable name="HeaderBar">
        <attr name="headerbar_t" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="OvalBg">
        <attr name="mult" format="float"/>
        <attr name="speed" format="float"/>
    </declare-styleable>
</resources>